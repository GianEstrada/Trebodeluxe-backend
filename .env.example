# Configuración del Backend de Treboluxe

# Puerto del servidor
PORT=5000

# URL del Frontend (cambiar por tu URL de Render)
FRONTEND_URL=https://trebodeluxe-front.onrender.com

# Configuración de CORS
CORS_ORIGIN=https://trebodeluxe-front.onrender.com

# Configuración de la base de datos (si usas una)
# DATABASE_URL=postgresql://user:password@localhost:5432/treboluxe

# JWT Secret (para autenticación)
JWT_SECRET=tu_jwt_secret_muy_seguro_aqui

# Configuración de Node
NODE_ENV=production
